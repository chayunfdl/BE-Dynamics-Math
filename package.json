{
  "name": "math-api",
  "version": "1.0.0",
  "description": "",
  "main": "./src/app.js",
  "type": "module",
  "scripts": {
    "start": "node app.js",
    "start:dev": "nodemon ./src/app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "vercel": "vercel"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@vercel/postgres": "^0.7.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "mysql2": "^3.9.1",
    "nanoid": "^5.0.5",
    "sequelize": "^6.36.0",
    "vercel": "^33.6.1"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "nodemon": "^3.0.3"
  },
  "vercel": {
    "version": 2,
    "builds": [
      {
        "src": "app.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/auth/(.*)",
        "dest": "auth.router.js"
      },
      {
        "src": "/api/user/(.*)",
        "dest": "users.router.js"
      },
      {
        "src": "/api/contact/(.*)",
        "dest": "contact-us.router.js"
      },
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ]
  }
}


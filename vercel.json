{
    "version": 2,
    "builds": [
        { "src": "build/app.js", "use": "@vercel/node" }
    ],
    "routes": [
        { "src": "/api/auth/(.*)", "dest": "build/routes/auth.router.js" },
        { "src": "/api/user/(.*)", "dest": "build/routes/users.router.js" },
        { "src": "/api/contact/(.*)", "dest": "build/routes/contact-us.router.js" },
        { "src": "/(.*)", "dest": "build/app.js" }
    ]
}